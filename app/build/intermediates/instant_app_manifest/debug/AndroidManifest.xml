        <activity android:name="com.example.prestamo.ClienteActivity" />
        <activity android:name="com.example.prestamo.PrincipalActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.prestamo.SecondActivity" />
        <activity android:name="com.example.prestamo.MainActivity" />
